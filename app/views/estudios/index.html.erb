<h1>Estudios Index</h1>

<p>
  <%= link_to 'Principal', controller: 'inicio' %> |
  <%= link_to 'Crear', new_estudio_path %>  
</p>

<table>

  <tr>
    <th>Nombre Estudio</th>
    <th>Topico</th>
  </tr>

  <% @estudios.each do |f| %>
    <tr>
      <td><%= f.nombre_estudio %></td>

      <td><%= link_to 'Ver', estudio_path(f) %></td>
      <td><%= link_to 'Editar', edit_estudio_path(f) %></td>
    </tr>

  <% end %>

</table>