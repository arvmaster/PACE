<h1>Preguntas cuestionario</h1>
<p>
  <%= link_to 'Principal', controller: 'inicio' %> |
  <%= link_to 'Crear', new_pregunta_cuestionario_path %>
</p>
  <table class="table table-striped table-bordered">
    <tr>
      <th>Valor</th>
      <th>Pregunta</th>
    </tr>
    <% @pregunta_cuestionarios.each do |f| %>
      <tr>
        <td><%= f.casilla ? "Si" : "No" %></td>
        <td><%= f.pregunta_cues %></td>
        <td><%= link_to 'Ver', pregunta_cuestionario_path(f) %></td>
        <td><%= link_to 'Editar', edit_pregunta_cuestionario_path(f) %></td>
      </tr>
    <% end %>
  </table>