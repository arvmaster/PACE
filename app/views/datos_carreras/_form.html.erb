<%= form_for @datos_carrera do |f| %>

      <div class="form-group">
        <%= f.label :Nombre_Carrera %></br>
        <%= f.text_field :nombre_carrera , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Matricula_Hombres %></br>
        <%= f.number_field :mat_h , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Matricula_Mujer %></br>
        <%= f.number_field :mat_m , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Area %></br>
        <%= f.text_field :area , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Nombre_Institución  %></br>
        <%= f.text_field :institucion  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Retencion_1°Año %></br>
        <%= f.number_field :ret_1a  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Puntaje_Ultimo_Matriculado %></br>
        <%= f.number_field :pun_last_ma  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Duración_Real %></br>
        <%= f.number_field :duracion_for  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Duración_Promedio %></br>
        <%= f.number_field :duracion_real  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Empleabilidad_1°Año %></br>
        <%= f.number_field :emp_1a  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Empleabilidad_2°Año %></br>
        <%= f.number_field :emp_2a  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Titulados_Hombres %></br>
        <%= f.number_field :tit_h  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Titulados_Mujeres %></br>
        <%= f.number_field :tit_m  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Ingreso_1°Año %></br>
        <%= f.number_field :ing_1a  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Ingreso_5°Año %></br>
        <%= f.number_field :ing_5a  , class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label :Ingreso_10°Año %></br>
        <%= f.number_field :ing_10a  , class: "form-control"%>
      </div>

    <%= f.submit 'Enviar', :class => 'btn btn-primary' %>
<% end %>
