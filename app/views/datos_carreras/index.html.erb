<h1>Datos Carrera</h1>

<p>
  <%= link_to 'Principal', controller: 'inicio' %> |
  <%= link_to 'Crear', new_datos_carrera_path %>
</p>

<table>

  <tr>
    <th>Nombre Carrera</th>
    <th>Topico</th>
  </tr>

  <% @datos_carreras.each do |f| %>
    <tr>
      <td><%= f.nombre_carrera %></td>
      <td><%= f.matricula_hombre %></td>
      <td><%= link_to 'Ver', datos_carrera_path(f) %></td>
      <td><%= link_to 'Editar', edit_datos_carrera_path(f) %></td>
    </tr>

  <% end %>

</table>